obj/ezlib obj/ezconst.a     obj/ezmisc.a      obj/ezstring.a   
          obj/ezwindows.a   obj/ezcontrols.a  obj/ezlineedit.a
          obj/ezalerts.a    obj/ezdisk.a      obj/ezdates.a
          obj/eztools.a     obj/ezmenu.a      obj/ezlist.a
          obj/ezcrypt.a     obj/ezdebug.a     obj/ezasm.a
          obj/ezcmdline.a
    {   delete obj/ezlib
        makelibiigs -l obj/ezlib -r obj/ezconst.a     obj/ezmisc.a      obj/ezstring.a   
        makelibiigs -l obj/ezlib -r obj/ezwindows.a   obj/ezcontrols.a  obj/ezlineedit.a
        makelibiigs -l obj/ezlib -r obj/ezalerts.a    obj/ezdisk.a      obj/ezdates.a
        makelibiigs -l obj/ezlib -r obj/eztools.a     obj/ezmenu.a      obj/ezlist.a
        makelibiigs -l obj/ezlib -r obj/ezcrypt.a     obj/ezasm.a       obj/ezasm.root
        makelibiigs -l obj/ezlib -r obj/ezcmdline.a   obj/ezdebug.a
     }

obj/ezconst.a
    unit/ezconst.unit
                        {   compile unit/ezconst.unit
                            move -c ezconst.int int
                            move -c ezconst.a obj }

obj/ezasm.a
    asm/ezasm.asm       {   compile asm/ezasm.asm
                            move -c ezasm.root obj
                            move -c ezasm.a obj }

obj/ezmisc.a
    unit/ezmisc.unit
    obj/ezconst.a
    obj/ezasm.a         {   compile unit/ezmisc.unit
                            move -c ezmisc.int int
                            move -c ezmisc.a obj }

obj/ezstring.a
    obj/ezconst.a
    unit/ezstring.unit  {   compile unit/ezstring.unit
